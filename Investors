    <script>
        function wrapLabel(str, max_width) {
            if (str.length <= max_width) return str;
            let wrapped = [];
            let words = str.split(' ');
            let current_line = '';
            for (let i = 0; i < words.length; i++) {
                let word = words[i];
                if ((current_line + word).length > max_width) {
                    wrapped.push(current_line.trim());
                    current_line = '';
                }
                current_line += word + ' ';
            }
            wrapped.push(current_line.trim());
            return wrapped;
        }

        const tooltipTitleCallback = (tooltipItems) => {
            const item = tooltipItems[0];
            let label = item.chart.data.labels[item.dataIndex];
            if (Array.isArray(label)) {
              return label.join(' ');
            } else {
              return label;
            }
        };
        
        const sharedChartOptions = {
            responsive: true,
            maintainAspectRatio: false,
            plugins: {
                legend: {
                    display: false
                },
                tooltip: {
                    callbacks: {
                        title: tooltipTitleCallback
                    }
                }
            }
        };

        const financialImpactData = {
            labels: ['R&D Productivity Loss', 'T3 Churn & Replacement Cost', 'SLA Penalties & Customer Churn'].map(label => wrapLabel(label, 16)),
            datasets: [{
                label: 'Annual Cost ($)',
                data: [1350000, 450000, 200000],
                backgroundColor: ['#0A9396', '#94D2BD', '#E9D8A6'],
                borderColor: ['#005F73', '#0A9396', '#CA6702'],
                borderWidth: 1
            }]
        };

        new Chart(document.getElementById('financialImpactChart'), {
            type: 'bar',
            data: financialImpactData,
            options: { ...sharedChartOptions,
                scales: {
                    y: {
                        beginAtZero: true,
                        ticks: {
                            callback: function(value, index, values) {
                                return '$' + (value / 1000000) + 'M';
                            }
                        }
                    }
                },
                plugins: { ...sharedChartOptions.plugins,
                    legend: { display: false },
                    title: {
                        display: true,
                        text: 'Annualized Hidden Costs of T3 Inefficiency',
                        font: { size: 16 },
                        color: '#333'
                    }
                }
            }
        });

        const createDoughnutChart = (id, percentage, color, label) => {
            new Chart(document.getElementById(id), {
                type: 'doughnut',
                data: {
                    labels: [label, 'Remaining'],
                    datasets: [{
                        data: [percentage, 100 - percentage],
                        backgroundColor: [color, '#e5e7eb'],
                        borderColor: [color, '#e5e7eb'],
                        borderWidth: 2,
                        circumference: 180,
                        rotation: 270,
                    }]
                },
                options: {
                     ...sharedChartOptions,
                     cutout: '70%',
                     plugins: {
                        ...sharedChartOptions.plugins,
                        tooltip: { enabled: false }
                     }
                },
                 plugins: [{
                    id: 'doughnut-center-text',
                    beforeDraw: function(chart) {
                        let width = chart.width,
                            height = chart.height,
                            ctx = chart.ctx;
                        ctx.restore();
                        let fontSize = (height / 114).toFixed(2);
                        ctx.font = "bold " + fontSize + "em Inter";
                        ctx.textBaseline = "middle";
                        let text = chart.data.datasets[0].data[0] + "%",
                            textX = Math.round((width - ctx.measureText(text).width) / 2),
                            textY = height / 1.5;
                        ctx.fillStyle = chart.data.datasets[0].backgroundColor[0];
                        ctx.fillText(text, textX, textY);
                        ctx.save();
                    }
                }]
            });
        };
        
        createDoughnutChart('mttrChart', 25, '#0A9396', 'MTTR Reduction');
        createDoughnutChart('t2EscalationChart', 25, '#EE9B00', 'T2 Escalation Reduction');
        createDoughnutChart('t3EscalationChart', 15, '#CA6702', 'T3 Escalation Reduction');
        createDoughnutChart('rndTimeChart', 20, '#BB3E03', 'R&D Time Reduction');

        new Chart(document.getElementById('marketGrowthChart'), {
            type: 'line',
            data: {
                labels: ['2023', '2024', '2025', '2026', '2027', '2028'],
                datasets: [
                    {
                        label: 'AIOps Market',
                        data: [13.56, 15.28, 17.22, 19.40, 21.85, 26.87],
                        borderColor: '#0A9396',
                        backgroundColor: 'rgba(10, 147, 150, 0.1)',
                        fill: true,
                        tension: 0.4
                    },
                     {
                        label: 'AI in Testing Market',
                        data: [1.9, 2.25, 2.67, 3.17, 3.76, 4.46],
                        borderColor: '#EE9B00',
                        backgroundColor: 'rgba(238, 155, 0, 0.1)',
                        fill: true,
                        tension: 0.4
                    }
                ]
            },
            options: {
                 ...sharedChartOptions,
                 plugins: {
                     ...sharedChartOptions.plugins,
                     legend: { display: true, position: 'bottom' }
                 },
                 scales: {
                    y: {
                        beginAtZero: true,
                        ticks: {
                            callback: function(value, index, values) {
                                return '$' + value + 'B';
                            }
                        }
                    }
                }
            }
        });
        
        new Chart(document.getElementById('marketSizeChart'), {
            type: 'bubble',
            data: {
                datasets: [
                    {
                        label: 'AIOps Platforms ($27B)',
                        data: [{x: 20, y: 30, r: 45}],
                        backgroundColor: 'rgba(10, 147, 150, 0.6)'
                    },
                    {
                        label: 'Intelligent Process Automation ($26B)',
                        data: [{x: 45, y: 55, r: 42}],
                        backgroundColor: 'rgba(148, 210, 189, 0.6)'
                    },
                    {
                        label: 'AI in Software Testing ($10B)',
                        data: [{x: 65, y: 25, r: 30}],
                        backgroundColor: 'rgba(233, 216, 166, 0.6)'
                    },
                     {
                        label: "Liberio's Niche ($7B+)",
                        data: [{x: 42, y: 35, r: 25}],
                        backgroundColor: 'rgba(238, 155, 0, 0.8)'
                    }
                ]
            },
            options: {
                ...sharedChartOptions,
                plugins: {
                     ...sharedChartOptions.plugins,
                     legend: { display: true, position: 'bottom' },
                     tooltip: {
                        callbacks: {
                            label: function(context) {
                                return context.dataset.label;
                            }
                        }
                     }
                 },
                 scales: {
                     x: { display: false },
                     y: { display: false }
                 }
            }
        });

    </script>
</body>
</html>
